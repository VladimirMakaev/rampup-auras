        "customText": "function()\n    \n    soundfile=\"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Sounds\\\\BananaPeelSlip.ogg\"\n    channel=\"Master\"\n    \n    cooldowns = {\n        {time = 5, text = \"Rapture Ramp\", sound = soundfile},\n        {time = 60, text = \"~30 seconds till Dread\", sound = soundfile},\n        {time = 80, text = \"Essence Ramp\", sound = soundfile},\n        {time = 120, text = \"Rapture Ramp + Eva\", sound = soundfile},\n        {time = 160, text = \"~20sec to 3rd Dread\", sound = soundfile}\n    }\n    \n    tval=ceil(GetTime() - aura_env.timer)\n    \n    for i=1, table.getn(cooldowns) do\n        if cooldowns[i].time == tval\n        then\n            if not aura_env.soundPlayed\n            then\n                \n                PlaySoundFile(cooldowns[i].sound, channel)\n                aura_env.soundPlayed=true\n            end\n            \n            return cooldowns[i].text\n        end        \n    end    \nend\n\n\n\n",
